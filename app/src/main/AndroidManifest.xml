        <activity android:name="com.zzg.zcib.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zzg.zcib.activity.RegisterActivity" />
        <activity android:name="com.zzg.zcib.activity.AddFriendActivity" />
        <activity android:name="com.zzg.zcib.activity.PersonActivity" />
